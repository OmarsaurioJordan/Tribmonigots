    case m_est_subir:
        zi += si_para_velocidad(especie) * dt;
        if zi &gt;= m_altuvuelo {
            zi = m_altuvuelo;
            estado = m_est_general;
        }
        break;
    case m_est_bajar:
        zi -= si_para_velocidad(especie) * dt;
        if zi &lt;= 0 {
            zi = 0;
            if s_mizona(x, y) != m_bioma_mar and !s_aveatenta(id, true) {
                var aux = instance_create(x, y, o_ave);
                aux.vida = vida;
                aux.especie = especie;
                instance_destroy();
                exit;
            }
            else {
                reloj_errar *= 0.5;
                estado = m_est_subir;
            }
        }
        break;
}

